{
  "name": "pilar",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "copy:assets": "cp -R projects/uiigateway/pilar/src/assets/ dist/uiigateway/pilar/assets/",
    "copy:readme": "cp README.md dist/uiigateway/pilar",
    "watch:lib": "ng build pilar --watch",
    "build:lib": "ng build --prod pilar && npm run copy:assets",
    "npm:pack": "npm run copy:readme && cd dist/uiigateway/pilar && npm pack",
    "package": "npm run build:lib && npm run npm:pack",
    "publish": "cd dist/uiigateway/pilar && npm publish --registry https://npm.uii.ac.id"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "^7.2.0",
