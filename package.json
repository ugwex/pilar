    "@angular/platform-browser-dynamic": "^7.2.0",
    "@angular/router": "^7.2.0",
    "@mdi/font": "^3.3.92",
    "@saritasa/ngx-text-mask": "^8.0.8",
    "@swimlane/ngx-datatable": "^13.0.0",
    "angular-highcharts": "^6.1.3",
    "awesome-bootstrap-checkbox": "^1.0.1",
    "compass-mixins": "^0.12.10",
